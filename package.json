{
  "name": "torrjs-redis-transport",
  "version": "0.0.1",
  "description": "a redis transport for torrjs gramework",
  "main": "lib/index.js",
  "repository": "git@github.com:ndY0/torrjs-redis-transport.git",
  "author": "ndy0",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc",
    "pretest": "rimraf coverage",
    "test": "node --trace-warnings node_modules/.bin/jest --detectOpenHandles",
    "test:perf": "node --max-old-space-size=6144 --expose-gc --trace-warnings node_modules/.bin/jest --runInBand --logHeapUsage --detectOpenHandles -c ./jest.perf.config.ts",
    "watch": "node --trace-warnings node_modules/.bin/jest --detectOpenHandles --watchAll"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/redis": "^2.8.28",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "redis": "^3.1.2"
  }
}
